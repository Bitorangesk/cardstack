<div class="your-info">
  <div class="your-photo" style="background-image:url(http://static1.squarespace.com/static/5238ec5be4b0bbcbb80e3ead/t/5331bf96e4b0c2ca4dcdd7b3/1395769249434/);"></div>
  <p>Hello, Frank</p>
  <p>Editor</p>
  <span class="account-options-list">
    <div {{action "openPlugins"}} class="account-options"><div>{{cs-svg "lightning" width="16px" height="22px"}}</div><span>Plugins</span></div>
    <div {{action "openDataSources"}} class="account-options"><div>{{cs-svg "data-sources" width="24px" height="19px" }}</div><span>Data Sources</span></div>
    <div class="account-options"><div>{{cs-svg "notifications" width="20px" height="21px"}}</div><span>Notifications</span></div>
    <div class="account-options"><div>{{cs-svg "settings" width="22px" height="22px"}}</div><span>Settings</span></div>
    <div class="account-options logout"><span>Logout</span> {{cs-svg "logout" width="22px" height="20px"}}</div>
  </span>
</div>

<div class="cs-admin-panel-container">
  {{#liquid-bind activePanel rules=flyout containerless=true class="cs-admin-panel" as |panel|}}
    {{#if panel}}
      {{component panel}}
    {{/if}}
  {{/liquid-bind}}
</div>