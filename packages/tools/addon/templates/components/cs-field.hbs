{{#mark-overlay id=id model=fieldInfo group="cardstack-fields" }}
  {{#if hasBlock}}
    {{yield (get content fieldName) }}
  {{else}}
    {{#if (is-component defaultRenderer)}}
      {{component defaultRenderer value=(get content fieldName) content=content fieldName=fieldName}}
    {{else}}
      {{get content fieldName}}
    {{/if}}
  {{/if}}
{{/mark-overlay}}
