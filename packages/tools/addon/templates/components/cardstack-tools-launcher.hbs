{{#if toolsAvailable}}
    {{#if hasBlock }}
      {{yield (hash active=active setActive=(action "setActive") toggleActive=(action "toggleActive") )}}
    {{else}}
      <div {{action "toggleActive"}}>
        {{svg-jar "cardstack-icon" width="26px" height="26px" class=(concat "cardstack-tools-launcher " (if active 'active'))}}
      </div>

    {{/if}}
{{/if}}
