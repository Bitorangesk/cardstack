{{#cs-collapsible-section class="cs-toolbox-section"
                          title=title
                          opened=opened
                          open=(action "open")
                          close=(action "close") }}

  <div class="cs-field-editor-section">
    {{#liquid-spacer}}
      {{#if (eq upstreamState "same")}}
        <div>Same as live version.</div>
      {{/if}}

      {{#if (eq upstreamState "different")}}
        <div>Differs from live version.</div>
      {{/if}}

    {{/liquid-spacer}}

  </div>

  {{#liquid-if enabled}}
    <footer class="cs-version-control-footer cs-field-editor-section">
      <a class="text-button" {{action "delete"}}>Delete</a>
      <button disabled={{eq modificationState "saved"}} {{action "update"}}>
        Update
      </button>
    </footer>
  {{/liquid-if}}

{{/cs-collapsible-section}}
