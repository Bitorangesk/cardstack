{{#cs-collapsible-section class="cs-toolbox-section"
                          title=title
                          opened=opened
                          open=(action "open")
                          close=(action "close") }}

  <div class="cs-field-editor-section">
    <div>Version: {{modelMeta.version}}</div>
    <div>Hash: {{modelMeta.hash}}</div>
    <div>Branch: {{modelMeta.branch}}</div>

    {{#unless fetchUpstreamModel.isRunning }}
      {{#if upstreamModel}}
        There's an upstream model.
      {{else}}
        There's no upstream model.
      {{/if}}
    {{/unless}}

  </div>

  {{#liquid-if enabled}}
    <footer class="cs-version-control-footer cs-field-editor-section">
      <a class="text-button" {{action "delete"}}>Delete</a>
      <button disabled={{not anythingPending}} {{action "update"}}>
        Update
      </button>
    </footer>
  {{/liquid-if}}

{{/cs-collapsible-section}}
